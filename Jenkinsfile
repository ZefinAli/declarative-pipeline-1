pipeline
{
    agent any
    
    stages
    {
        stage ('Build')
        {
            steps
            {
                echo 'Build App'
            }
        }

        stage ('stage')
        {
            steps
            {
                echo 'staging App'
            }
        }

        stage ('deploy')
        {
            steps
            {
                echo 'deploy App'
            }
        }

    }
}
